public class Largest {

    public static void main(String[] args) {

        double b1 = 5, b2 = 50, b3 = 150;

        if (b1 >= b2 && b1 >= b3)
            System.out.println(b1 + " je najveci broj.");

        else if (b2 >= b1 && b2 >= b3)
            System.out.println(b2 + " je najveci broj.");

        else
            System.out.println(b3 + " je najveci broj.");
    }
}
